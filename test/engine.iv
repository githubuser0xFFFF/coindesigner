#Inventor V2.1 ascii


DEF root Separator {

  DEF engine1 Separator {

    Info {
      string "Conection of a field from a dragger, using an intermediate engine"

    }
    DEF drg1+0 Scale2Dragger {
      scaleFactor 1 1 1

    }
    Translation {
      translation 2 0 0

    }
    Cone {
      bottomRadius =
        DEF engine+1 DecomposeVec3f { vector = USE drg1+0 . scaleFactor } . x
      height = USE engine+1      . y

    }
  }
  DEF text Separator {

    Translation {
      translation 1 -2 0

    }
    DEF dragger_text Text2 {
      string = USE drg1+0      . scaleFactor
      justification CENTER

    }
    Translation {
      translation 0 -0.5 0

    }
    DEF timer_text Text2 {
      string  = ElapsedTime { speed 1 }      . timeOut
      justification CENTER

    }
  }
}
